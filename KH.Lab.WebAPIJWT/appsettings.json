{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "UrlConnection": "localhost:6379",
    "Expire": "30",
    "KeyPrefix": "Presco:Test"
  },
  "Cookie": {
    "Expire": "30",
    "KeyPrefix": ".Presco.Test"
  },
  "JWT": {
    "ValidAudience": "http://localhost:7130",
    "ValidIssuer": "http://localhost:7130",
    "Secret": "JWTAuthentication@777"
  },
  "JsonWebTokenKeys": {
    "ValidateIssuerSigningKey": true,
    "IssuerSigningKey": "JWTAuthentication@777",
    "ValidateIssuer": true,
    "ValidIssuer": "https://localhost:7130",
    "ValidateAudience": true,
    "ValidAudience": "https://localhost:7130",
    "RequireExpirationTime": true,
    "ExpirationTime": 30,
    "ValidateLifetime": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=JWT;Trusted_Connection=True;MultipleActiveResultSets=true;"
  }
}
